{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": { "maxDuration": 30 }
      },
      {
        "src": "dist/s3-pdf/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/pdfs/?",
        "dest": "/server.js",
        "methods": ["GET"],
        "headers": {
          "cache-control": "public, max-age=300, s-maxage=300",
          "content-type": "application/json"
        }
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/s3-pdf/assets/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.+?)\\.(js|css|png|jpg|jpeg|gif|svg|ico|pdf|woff|woff2|ttf|eot)$",
        "dest": "/dist/s3-pdf/$1.$2",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/dist/s3-pdf/index.html",
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate"
        }
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }