{
    "builds": [
      { "src": "frontend/dist/your-angular-app/**", "use": "@vercel/static" },
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1.js" },
      { "src": "/(.*)", "dest": "/frontend/dist/your-angular-app/$1" }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
  